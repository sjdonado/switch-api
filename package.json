{
  "name": "switch-api",
  "version": "1.0.0",
  "description": "Serverless cloud functions",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "update": "yarn --cwd dist/src install",
    "prebuild": "rimraf dist",
    "build": "babel . --out-dir dist --ignore node_modules && copyfiles src/package.json dist",
    "predeploy": "yarn run update",
    "deploy": "yarn --cwd dist/src start",
    "prelocal-deploy": "functions start",
    "local-deploy": "functions deploy switchDev -S dist/src --trigger-http",
    "debug": "functions debug switchDev",
    "logs": "functions logs read -S dist/src"
  },
  "author": "sjdonado",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "copyfiles": "^2.1.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "rimraf": "^2.6.2"
  }
}
